<h1>Our Beverages</h1>
<div class="product-grid">
  <% items.forEach(item => { %>
    <div class="product-card">
      <img src="<%= item.image %>" alt="<%= item.name %>">
      <h3><%= item.name %></h3>
      <a href="/items/<%= item.slug %>" class="btn">View Details</a>
    </div>
  <% }) %>
</div>

